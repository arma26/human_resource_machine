--For each thing in the inbox, send to the outbox the
--total number of matching items on the floor.

--19 = counter
--18 = iterator
--17 = inbox
--14 = stop

setup:
  COPYFROM 14
  COPYTO 19
  COPYTO 18
main:
  INBOX
  COPYTO 17
count:
  COPYFROM [18]
  JUMPZ zero
  SUB 17
  JUMPZ equal
  BUMPUP 18
  JUMP count
equal:
  BUMPUP 19
  BUMPUP 18
  COPYFROM 17
  JUMP count
zero:
  COPYFROM 19
  OUTBOX
  JUMP setup
